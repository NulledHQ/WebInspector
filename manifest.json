{
  "manifest_version": 3,
  "name": "WebInspector",
  "short_name": "WebInspector",
  "version": "1.0.0",
  "description": "Inspect, filter, and replay network requests directly with better UI inside Chrome DevTools.",
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "html/devtools.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "NulledHQ",
  "homepage_url": "https://github.com/NulledHQ/WebInspector",
  "minimum_chrome_version": "100"
}
